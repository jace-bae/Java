package basic;

public class basic_18_rectangle {
	int width;
	int height;
	
	public double getArea() {
		return width*height;
	}
	
	public static void main(String[] args) {
		basic_18_rectangle rect = new basic_18_rectangle();
		rect.width = 4;
		rect.height = 5;
		System.out.println("사각형의 면적은" + rect.getArea());

	}

}
